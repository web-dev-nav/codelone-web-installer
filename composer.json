{
    "name": "codelone/laravel-web-installer",
    "description": "Advanced web installer for Laravel applications with license verification and database schema import capabilities",
    "keywords": ["laravel", "installer", "license", "schema-import", "web-installer", "setup", "wizard"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CodeLone",
            "email": "info@codelone.dev",
            "homepage": "https://codelone.dev"
        }
    ],
    "homepage": "https://github.com/codelone/laravel-web-installer",
    "support": {
        "issues": "https://github.com/codelone/laravel-web-installer/issues",
        "source": "https://github.com/codelone/laravel-web-installer"
    },
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
        "filament/filament": "^2.0|^3.2",
        "livewire/livewire": "^2.0|^3.0",
        "spatie/laravel-package-tools": "^1.14",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.4",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CodeLone\\LaravelWebInstaller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeLone\\LaravelWebInstaller\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeLone\\LaravelWebInstaller\\WebInstallerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
}