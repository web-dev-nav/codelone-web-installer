{
    "name": "codehive/laravel-web-installer",
    "description": "Advanced web installer for Laravel applications with license verification and database schema import capabilities",
    "keywords": ["laravel", "installer", "license", "schema-import", "web-installer", "setup", "wizard"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CodeHive",
            "email": "info@codehive.dev",
            "homepage": "https://codehive.dev"
        }
    ],
    "homepage": "https://github.com/codehive/laravel-web-installer",
    "support": {
        "issues": "https://github.com/codehive/laravel-web-installer/issues",
        "source": "https://github.com/codehive/laravel-web-installer"
    },
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0",
        "filament/filament": "^3.0",
        "livewire/livewire": "^3.0",
        "spatie/laravel-package-tools": "^1.14",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CodeHive\\LaravelWebInstaller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeHive\\LaravelWebInstaller\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeHive\\LaravelWebInstaller\\WebInstallerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}